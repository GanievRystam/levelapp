{
  "expo": {
    "name": "levelcharacter",
    "slug": "levelcharacter",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "levelcharacter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.levelcharacter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "build": {
        "cssFile": "./assets/global.css"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
